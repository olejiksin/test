}\r\n    }\r\n}\r\n\r\nexport default function RegPage(props) {\r\n\r\n    const initState = {\r\n        email: null,\r\n        password: null\r\n    };\r\n\r\n\r\n    const [state, dispatch] = useReducer(reducer, initState);\r\n\r\n    const onChange = (type, data) => {\r\n        dispatch({type: type, payload: data});\r\n    };\r\n\r\n    const reg = () => {\r\n        let data = {\r\n            email: state.email,\r\n            password: state.password\r\n        };\r\n        axios.post('/reg', data)\r\n            .then(() => {\r\n                props.history.push('/login')\r\n            })\r\n            .catch((er) => {\r\n                props.history.push('/reg');\r\n                console.log(er);\r\n                alert('Some error');\r\n            })\r\n    };\r\n\r\n    return (\r\n        <div>\r\n            <div className={'reg'}>\r\n                <input className={'text-input'} type={'text'} name={\"email\"} id={\"email\"}\r\n                       onChange={(event) => onChange('email', event.target.value)}/>\r\n                <label className={'log-label'}>Password</label>\r\n                <br/>\r\n                <input className={'text-input'} type={'password'} name={\"password\"} id={\"password\"}\r\n                       onChange={(event) => onChange('password', event.target.value)}/>\r\n                <Link to={'/login'} className={\"log-reg-btn\"}>To log in</Link>\r\n                <button className={\"log-reg-btn\"} onClick={() => reg()}>Sign up</button>\r\n            </div>\r\n        </div>);\r\n}\r\n\r\n\r\n"},"lineCount":null}},"error":null,"hash":"ef365cfe72e16332e710e4daaf7c3b36","cacheData":{"env":{}}}